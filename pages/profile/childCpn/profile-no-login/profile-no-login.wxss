@import "../../../../assets/iconfont/iconfont.wxss";
.no-login{
  padding: 100rpx 50rpx;
  box-sizing: border-box;
}

.title{
  font-size: 56rpx;
  text-align: center;
}

.to-login{
  background-color: rgb(9, 187, 7);
  color: #fff;
  line-height: 48rpx;
  padding: 24rpx 100rpx;
  box-sizing: border-box;
  border-radius: 50rpx;
  margin-top: 100rpx;
  text-align: center;
}

.icon-weixin{
  margin-right: 26rpx;
  font-size: 50rpx;
}

.wx-login{
  font-size: 38rpx;
}

.icon-hengxian{
  margin-top: 130rpx;
  text-align: center;
  color: #ccc;
  font-size: 46rpx;
  font-weight: 400;
}

.other{
  text-align: center;
  font-size: 32rpx;
  margin-top: 50rpx;
  color: #aaa;
}

.login-list{
  margin-top: 100rpx;
  display: flex;
  justify-content: space-around;
  align-items: center;
}

.login-list .iconfont{
  font-size: 90rpx;
  color: rgb(124, 124, 124);
}

.login-list .text{
  margin-top: 18rpx;
  text-align: center;
  color: #666;
  font-size: 26rpx;
}



